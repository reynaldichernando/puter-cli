{
  "name": "puter-cli",
  "version": "1.8.6",
  "description": "Command line interface for Puter cloud platform",
  "main": "index.js",
  "bin": {
    "puter": "./bin/index.js"
  },
  "preferGlobal": true,
  "type": "module",
  "scripts": {
    "start": "node bin/index.js",
    "test": "TZ=UTC vitest run tests/*",
    "test:watch": "TZ=UTC vitest --watch tests/*",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "coverage": "TZ=UTC vitest run --coverage"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "puter",
    "cli",
    "cloud"
  ],
  "author": "Ibrahim.H",
  "license": "MIT",
  "dependencies": {
    "@heyputer/puter.js": "^2.1.2",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^13.0.0",
    "conf": "^12.0.0",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.4.7",
    "glob": "^11.0.0",
    "inquirer": "^9.2.12",
    "minimatch": "^10.0.1",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "uuid": "^11.0.5",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "2.1.8",
    "auto-changelog": "^2.5.0",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeyPuter/puter-cli.git"
  },
  "bugs": {
    "url": "https://github.com/HeyPuter/puter-cli/issues"
  },
  "homepage": "https://github.com/HeyPuter/puter-cli",
  "packageManager": "pnpm@8.15.9+sha512.499434c9d8fdd1a2794ebf4552b3b25c0a633abcee5bb15e7b5de90f32f47b513aca98cd5cfd001c31f0db454bc3804edccd578501e4ca293a6816166bbd9f81"
}
